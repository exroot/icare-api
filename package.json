{
  "name": "icare-api",
  "version": "0.0.1",
  "description": "iCare RESTful API",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build": "tsc --project ./",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrations": "typeorm migration:create -n BaseMigration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exroot/icare-api.git"
  },
  "keywords": [
    "iCare",
    "REST",
    "API"
  ],
  "author": "exroot (Elias Torrealba)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/exroot/icare-api/issues"
  },
  "homepage": "https://github.com/exroot/icare-api#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "faker": "^5.5.3",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql": "^2.14.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.36",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/faker": "^5.5.9",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^8.0.29",
    "@types/winston": "^2.4.4",
    "nodemon": "^2.0.14",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  }
}
